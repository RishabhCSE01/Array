/ find large element in a array
#include<bits/stdc++.h>
using namespace std;
void large(int arr[],int n){
    int max=arr[0];
    for(int i=0;i<n;i++){
      if(arr[i]>max){
        max=arr[i];
      }
    }
    cout<<"large element is: "<<max;
}

int main(){
    int arr[] = {5,6,3,6,4,10};
    int n=6;
    large(arr,n);
}
